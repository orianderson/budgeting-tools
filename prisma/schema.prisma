datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

generator client {
    provider             = "prisma-client-py"
    recursive_type_depth = 5
}

model Supplies {
    id          String @id @default(uuid())
    code        String
    description String
    unit        String
    price       Float

    @@map("supplies")
}
