datasource db {
    provider = "sqlite"
    url      = "file:database.db"
}

generator client {
    provider             = "prisma-client-py"
    recursive_type_depth = 5
}

model Insumos {
    id          String @id @default(uuid())
    code        String
    description String
    price       Float

    @@map("insumo")
}
